    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "^2.1.14",
    "react-addons-test-utils": "^15.5.1",
    "redbox-react": "^1.3.6",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
